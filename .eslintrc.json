{
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": [2,"never"],
    "semi": [2,"never"],
    "max-len": 0,
    "arrow-body-style": [2, "as-needed"]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  }
}